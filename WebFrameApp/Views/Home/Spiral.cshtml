@model WebFrameApp.Models.SpiralModel

@{
    ViewBag.Title = "Spiral";
}

@using (Html.BeginForm())
{
    @Html.AntiForgeryToken()

    <div class="form-horizontal">
        <h4>Spiral</h4>
        <br />
        <hr />
        <br />
        <div>
            <div class="form-group">
                @Html.LabelFor(model => model.ordered, new { @class = "control-label col-md-2" })
                <div class="col-md-10">
                    @Html.EditorFor(model => model.ordered)
                    @Html.ValidationMessageFor(model => model.ordered)
                </div>
            </div>
            <div class="form-group">
                @Html.LabelFor(model => model.result, new { @class = "control-label col-md-2" })
                <div class="col-md-10">
                    @Html.DisplayFor(model => model.result)

                </div>
            </div>
            <div class="form-group">
                <div class="col-md-offset-2 col-md-10">
                    <input type="submit" value="Run" class="btn btn-default" />
                </div>
            </div>
        </div>

    </div>
}

<br />
<br />
@section Scripts {
}
